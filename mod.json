{
    "geode": "4.3.1",
    "gd": {
        "android": "2.2074",
        "win": "2.2074",
        "mac": "2.2074",
        "ios": "2.2074"
    },
    "version": "v1.12.0",
    "id": "hiimjustin000.more_icons",
    "name": "More Icons",
    "developer": "hiimjasmine00",
    "description": "A mod that loads custom icons.",
    "dependencies": {
        "geode.node-ids": ">=v1.17.0",
        "geode.texture-loader": {
            "importance": "suggested",
            "version": ">=v1.7.0"
        }
    },
    "resources": {
        "sprites": [
            "resources/*.png"
        ]
    },
    "api": {
        "include": [
            "include/*.hpp"
        ]
    },
    "settings": {
        "debug-logs": {
            "name": "Debug Logs",
            "description": "Whether to enable debug logs in the mod.",
            "type": "bool",
            "default": true
        },
        "traditional-packs": {
            "name": "Load from Traditional Packs",
            "description": "Whether to load icons from traditional icon packs.",
            "type": "bool",
            "default": true
        }
    },
    "links": {
        "community": "https://discord.gg/QVKmbvBXA7",
        "source": "https://github.com/hiimjasmine00/MoreIcons",
        "homepage": "https://more-icons.hiimjasmine00.com"
    },
    "tags": [
        "content",
        "customization",
        "enhancement",
        "gameplay",
        "interface",
        "modtober24",
        "offline"
    ]
}
